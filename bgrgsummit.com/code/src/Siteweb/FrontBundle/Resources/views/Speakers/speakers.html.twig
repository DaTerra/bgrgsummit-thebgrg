          <div class="col1of3 autocontent autocontent220">
            <a name="Dominique" class="center circular mb-15" style="background: url({{asset('bundles/sitewebfront/img/Dominique.jpg')}}) no-repeat;"></a>
            <h3 class="titley">Dominique Jackson</h3>
            <div class="yellowBox">
              <div class="col12of12 content">
                <!--
                <p><strong>Presentation Title</strong></p>
                <p>aaa</p>
